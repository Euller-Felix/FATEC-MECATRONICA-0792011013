# ler todos os nomes que o usuario quiser escrever
# Exibir todos os nomes em ordem alfabetica
nomes = []

continuar = True

while continuar == True:
  nome = input("informe um nome:")
  nomes.append(nome)
  print(nomes)
  if input("deseja continuar? (Sim/NÃ£o)") == 'Sim':
    continuar = True
  else:
    continuar = False
  
